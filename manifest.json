{
  "manifest_version": 3,
  "name": "SummarizePro",
  "description": "An extension to summarize articles and youtube videos",
  "version": "1.0",
  "permissions": ["activeTab", "declarativeContent"],
  "host_permissions": ["http://127.0.0.1:5000/*"],
  "action": {
    "default_title": "Summarize this video",
    "default_popup": "youtube.html"
  }

}